{
  "name": "luler/phpcrypt",
  "description": "这是一个PHP数据加密解密包，可以设置密码、过期时间，在一些需要对数据进行加密解密传输的应用中非常有用",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "luler",
      "email": "1207032539@qq.com",
      "homepage": "https://github.com/luler",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0"
  },
  "autoload": {
    "psr-4": {
      "Php\\Crypt\\": "src/Php/Crypt"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  }
}
